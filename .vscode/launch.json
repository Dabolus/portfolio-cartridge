  "version": "0.2.0",
  "configurations": [
    {
      "type": "emulicious-debugger",
      "request": "launch",
      "name": "Launch in Emulicious (Game Boy)",
      "emuliciousPath": "/Applications/Emulicious.app/Emulicious.jar",
      "program": "${workspaceFolder}/build/gb/portfolio.gb",
      "port": 58870,
      "stopOnEntry": true,
      "preLaunchTask": "Game Boy - Watch"
    },
    {
      "type": "emulicious-debugger",
      "request": "launch",
      "name": "Launch in Emulicious (Game Boy Color)",
      "emuliciousPath": "/Applications/Emulicious.app/Emulicious.jar",
      "program": "${workspaceFolder}/build/gbc/portfolio.gbc",
      "port": 58870,
      "stopOnEntry": true,
      "preLaunchTask": "Game Boy Color - Watch"
    },
    {
      "type": "emulicious-debugger",
      "request": "launch",
      "name": "Launch in Emulicious (Sega Master System)",
      "emuliciousPath": "/Applications/Emulicious.app/Emulicious.jar",
      "program": "${workspaceFolder}/build/sms/portfolio.sms",
      "port": 58870,
      "stopOnEntry": true,
      "preLaunchTask": "Sega Master System - Watch"
    },
    {
      "type": "emulicious-debugger",
      "request": "launch",
      "name": "Launch in Emulicious (Game Gear)",
      "emuliciousPath": "/Applications/Emulicious.app/Emulicious.jar",
      "program": "${workspaceFolder}/build/gg/portfolio.gg",
      "port": 58870,
      "stopOnEntry": true,
      "preLaunchTask": "Game Gear - Watch"
    }
  ]
